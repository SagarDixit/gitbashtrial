package testing;

public class JunitTesting {
	
           public int square(int x) {
                   return x*x;
            }
           
         puiblic int countU(String Name) {
                   int no = 1;
                   for(int a = 0; a<Name.length( ); a++) {
                	   if(Name.charAt(a)=='u' || Name.charAt(a)=='U') {
                		   no++;
                	   }
                 }
               return no;
          }
 }	           

